# .gitignore for Visual Studio Code

# Ignore node_modules directory
node_modules/

# Ignore logs
logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Ignore environment variable files
.env
.env.local
.env.development.local
.env.test.local
.env.production.local

# Ignore build output
dist/
build/

# Ignore coverage directory
coverage/

# Ignore dependency directories
jspm_packages/

# Ignore IDE specific files
.vscode/
.idea/

# Ignore OS specific files
.DS_Store
Thumbs.db

# Ignore lock files
package-lock.json
yarn.lock

# Ignore miscellaneous
*.sublime-project
*.sublime-workspace
*.code-workspace